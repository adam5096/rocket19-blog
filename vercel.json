{
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=31536000, immutable"
                }
            ]
        }
    ],
    "build": {
        "env": {
            "NODE_ENV": "production"
        },
        "buildCommand": "npm install && npm run docs:build",
        "outputDirectory": ".vitepress/dist"
    }
}